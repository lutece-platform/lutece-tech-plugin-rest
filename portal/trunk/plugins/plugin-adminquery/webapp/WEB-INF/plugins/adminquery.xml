<?xml version="1.0" encoding="ISO-8859-1"?>
<plug-in>
    <name>adminquery</name>
    <class>fr.paris.lutece.portal.service.plugin.PluginDefaultImplementation</class>
    <version>2.0</version>
    <description>adminquery.plugin.description</description>
    <provider>adminquery.plugin.provider</provider>
    <provider-url>http://lutece.paris.fr</provider-url>
    <icon-url>images/admin/skin/plugins/adminquery/adminquery.png</icon-url>
    <copyright>Copyright (c) 2001-2006 Mairie de Paris</copyright>

    <!-- Le plugin requiert-il un pool de connexions paramétrable : 1 - oui, 0 - non -->
    <db-pool-required>1</db-pool-required>

    <!-- Listes des fonctionnalitées d'administration éventuellement introduites par le plugin-->
    <admin-features>
        <admin-feature>
            <feature-id>ADMIN_QUERY_MANAGEMENT</feature-id>
            <feature-title>adminquery.adminFeature.adminquery_management.name</feature-title>
            <feature-description>adminquery.adminFeature.adminquery_management.description</feature-description>
            <feature-level>1</feature-level>
            <feature-url>jsp/admin/plugins/adminquery/ManageAdminQuery.jsp</feature-url>
        </admin-feature>
    </admin-features>

</plug-in>
