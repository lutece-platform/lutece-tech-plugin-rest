<?xml version="1.0" encoding="ISO-8859-1"?>

<plug-in>
    
    <!-- Plugin Informations -->
    <name>formengine</name>
    <class>fr.paris.lutece.portal.service.plugin.PluginDefaultImplementation</class>
    <version>2.0</version>
    <description>Module de gestion de formulaires HTML</description>
    <provider>Mairie de Paris</provider>
    <provider-url>http://lutece.paris.fr</provider-url>
    <icon-url>images/admin/skin/plugins/form/form.png</icon-url>
    <copyright>Copyright 2001-2006 Mairie de Paris</copyright>
    <db-pool-required>0</db-pool-required>
    
    <!-- Specific plugin CSS stylesheet -->
    <css-stylesheet>formengine/formengine.css</css-stylesheet>
    
    <!-- Applications -->
    <applications>
        <application>
            <application-id>formengine</application-id>
            <application-class>fr.paris.lutece.plugins.formengine.web.FormEngineApp</application-class>
        </application>
    </applications>
    
    
</plug-in>

