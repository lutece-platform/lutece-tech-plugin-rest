<?xml version="1.0" encoding="ISO-8859-1"?>

<plug-in>
    <name>codewizard</name>
    <class>fr.paris.lutece.portal.service.plugin.PluginDefaultImplementation</class>
    <version>2.0</version>
    <description>Assistant de génération de code Java pour objets métiers Lutece</description>
    <documentation></documentation>
    <installation></installation>
    <changes></changes>
    <user-guide></user-guide>
    <provider>Mairie de Paris</provider>
    <provider-url>http://lutece.paris.fr</provider-url>
    <icon-url>images/admin/skin/plugins/codewizard/codewizard.png</icon-url>
    <copyright>Copyright 2001-2006 Mairie de Paris</copyright>
    <db-pool-required>0</db-pool-required>
    
       
    <!-- Specific plugin CSS stylesheet -->
    <css-stylesheet>codewizard/codewizard.css</css-stylesheet>
    
    <!-- Applications -->
    <applications>
        <application>
            <application-id>codewizard</application-id>
            <application-class>fr.paris.lutece.plugins.codewizard.web.CodeWizardApp</application-class>
        </application>
    </applications>
</plug-in>
